  $(".huzi-accordion-warp .huzi-ac-title").click(function(){
    if($(this).next(".huzi-ac-body").hasClass("active")){
      $(this).next(".huzi-ac-body").removeClass("active").slideUp();
      $(this).removeClass("active");
    } else {
      $(".huzi-accordion-warp .huzi-ac-title").removeClass("active");
      $(".huzi-ac-item .huzi-ac-body").removeClass("active").slideUp();
      $(this).next(".huzi-ac-body").addClass("active").slideDown();
      $(this).addClass("active");
    }
  });

  // Input script
  $(".huzi-input-warp .huzi-input").on('focus',function(){
    var all_input = $(".huzi-input-warp .huzi-input");
    for (i = 0; i < all_input.length; ++i) {
      if( all_input[i].value == '' ){
        $(all_input[i]).parent().removeClass("active");
      }
    }
    //         $(".huzi-input-warp .huzi-input").removeClass("active");
    //         $(this).addClass("active");
    $(this).parent().addClass("active");
  });

  $(window).on('load', function(){
    var all_input = $(".huzi-input-warp .huzi-input");
    for (i = 0; i < all_input.length; ++i) {
      if( all_input[i].value == '' ){
        $(all_input[i]).parent().removeClass("active");
      }else{
        $(all_input[i]).parent().addClass("active");
      }
    }
  });

  //tabs
  $(function () {
    $('.tab-button').click(function () {
      // Check for active
      $('.tab-button').removeClass('active');
      $(this).addClass('active');
      // Display active tab
      let currentTab = $(this).attr('href');
      $('.tab-content').hide();
      $(currentTab).show();
      return false;
    });
  });

  $("#huzi-faq-page-list").on('change', function(){
    var list_val = $(this).val();
    $('.nws-faqs .tab-contents .tab-content').css('display','none');
    $(list_val).css('display','block');
  });

  $(".nws-pdp-variants .nws-pdp-variant").click(function(){
    $(".nws-pdp-variants .nws-pdp-variant").removeClass('nws-active');
    $(this).addClass('nws-active');
    var val = $(this).attr("data-val");
    var id = $(this).attr("data-id");
    var img = $(this).attr("data-img");
    $(".nws-btn-pdp").attr('href','/cart/'+id+':1');
    $(".nws-selection-name span").text(val);
    $('.nws-product-images.nws-pdp-option').addClass('nws-act-img');
    $('.nws-product-images.nws-pdp-option img').removeClass('nws-active-img');
    $('.nws-product-images.nws-pdp-option img[data-option="'+img+'"]').addClass('nws-active-img');
  });

