<!-- nws-faqs -->
<style>
.breadcrumbs li, .breadcrumbs li a, .breadcrumbs li:before {
    display: inline-block;
    vertical-align: middle;
    text-transform: uppercase;
    font-size: 12px;
}
.breadcrumbs a:not(:hover) {
    color: #000;
}  
.nws-tabs .breadcrumbs {
    padding-left: 75px;
}  
.nws-faqs .nws-tabs {
    border: 1px solid #F4DFDE;
    background-color: #FFF3F2;
}
.nws-tab-item h4 {
    font-family:"futura-pt";
    font-size: 20px;
    text-align: center;
    padding: 20px 20px 40px;
    font-weight: 500;
    color: #000;
} 
.tab-button.active h4 {
    color: #F4B0B1;
}  
.nws-tab-row .tab-button.active {
    border: 3px solid #F4B0B1;
    border-radius: 10px;
}  
.tab-button.active .nws-svg svg path {
    stroke: #F4B0B1;
}  
.tab-button.active .nws-svg.svg2 svg path {
    stroke: #F4B0B1;
    fill: #F4B0B1;
}  
.huzi-ac-title {
    font-family:"playfair-display";
    margin: 0;
    cursor: pointer;
    user-select: none;
    position: relative;
    font-weight: 600;
    font-size: 16px;
    line-height: 1.4;
    color: #43494e;
    padding-right: 25px;
}  
.huzi-ac-title.active {
    color: #4C1E1E;
} 
.huzi-accordion-warp .huzi-ac-item .huzi-ac-body {
    padding: 18px 0;
    overflow: hidden;
    transition: max-height .2s ease-out;
    display: none;
    font-size: 17px;
}  
.huzi-ac-title.active:after {
    content: "";
    background: #4C1E1E;
    display: block;
    width: 13px;
    height: 2px;
    position: absolute;
    right: 0;
    top: 50%;
}  
.nws-bottom-text h2 {
    color: #000;
}
.huzi-ac-title:after {
    transform: translateY(-45%);
    font-size: 25px;
    font-weight: 600;
}  
.tab-contents .nws-btn-one {
    background: #F7BCBD;
    color: #4C1E1E;
    max-width: 405px;
    margin: auto;
}
.tab-contents .nws-btn-one:hover {
  background: #4C1E1E;
  color: #fff;
}  
</style>

